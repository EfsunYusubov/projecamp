@using EntityLayer.Concrete
@model List<AppUser>
@{
    ViewData["Title"] = "UserRoleList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>İstifadəçi Role Listesi</h1>

<table class="table table-bordered">
    <tr>
        <th>İstifadəçi</th>
        <th>Rol ata</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td><a href="/Admin/AdminRole/AssignRole/@item.Id" class="btn btn-outline-twitter">Rol ata</a></td>
        </tr>
    }
</table>
<a href="/Admin/AdminRole/Index/"class="btn btn-outline-primary">Rol Listesine geri don</a>

